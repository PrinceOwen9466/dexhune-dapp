{
  "name": "dexhune-dapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn run vite --host\" \"yarn run dev:css\"",
    "dev:css": "npx tailwindcss -i src/css/tailwind/config.css -o src/css/tailwind/index.css --watch",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@web3modal/ethers": "^3.5.0",
    "@web3modal/wallet": "^3.5.0",
    "ethers": "^6.9.0",
    "pinia": "^2.1.7",
    "primevue": "^3.44.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "web3": "^4.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/cli-plugin-e2e-cypress": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "cypress": "^9.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}
